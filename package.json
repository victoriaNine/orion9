{
  "name": "orion9",
  "version": "2.0.0",
  "description": "",
  "author": "Victoria Nine",
  "scripts": {
    "dev": "cross-env NODE_ENV=development babel-node ./node_modules/webpack-dev-server/bin/webpack-dev-server --inline --hot --progress --inspect",
    "prod": "cross-env NODE_ENV=production babel-node ./node_modules/webpack-dev-server/bin/webpack-dev-server --progress",
    "start": "serve dist -s -c 1",
    "prestart": "npm run build",
    "build": "cross-env NODE_ENV=production babel-node ./node_modules/webpack/bin/webpack --progress",
    "prebuild": "mkdirp dist",
    "test": "npm run -s lint && jest --coverage",
    "test:watch": "npm run -s test -- --watch",
    "lint": "eslint src test",
    "copyModules": "babel-node internal/copyModules.js"
  },
  "jest": {
    "setupFiles": [
      "./test/setup.js"
    ],
    "testURL": "http://localhost:9000",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy",
      "^react$": "preact-compat",
      "^react-dom$": "preact-compat"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.2",
    "babel-jest": "^20.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-block-scoping": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.24.0",
    "babel-runtime": "^6.11.6",
    "chai": "^4.0.2",
    "copy-webpack-plugin": "^4.2.0",
    "core-js": "^2.4.1",
    "cross-env": "^5.1.0",
    "css-loader": "^0.28.0",
    "dom4": "1.8.3",
    "enhanced-resolve": "^3.4.1",
    "eslint": "^4.9.0",
    "eslint-plugin-jest": "^20.0.0",
    "eslint-plugin-react": "^7.0.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.0",
    "json-loader": "^0.5.4",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "postcss-loader": "^2.0.8",
    "preact-jsx-chai": "^2.2.1",
    "raw-loader": "^0.5.1",
    "regenerator-runtime": "^0.10.3",
    "replace-bundle-webpack-plugin": "^1.0.0",
    "script-ext-html-webpack-plugin": "^1.8.7",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.9.0",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.1",
    "url-loader": "^0.6.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "copy": "^0.3.1",
    "gsap": "^1.20.3",
    "imagemin-webpack-plugin": "^1.5.2",
    "lodash": "^4.17.4",
    "offline-plugin": "^4.8.4",
    "postcss-cssnext": "^3.0.2",
    "preact": "^8.1.0",
    "preact-compat": "^3.15.0",
    "preact-render-to-string": "^3.6.0",
    "preact-router": "^2.5.1",
    "precss": "^2.0.0",
    "promise-polyfill": "^6.0.2",
    "proptypes": "^1.0.0",
    "react-router-dom": "^4.2.2",
    "resource-hints-webpack-plugin": "^0.0.1",
    "serve": "^6.3.1",
    "stats.js": "^0.17.0",
    "webpack-md5-hash": "^0.0.5"
  }
}
